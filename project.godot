; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=3

[android]

modules="org/godotengine/godot/FireBase,org/godotengine/godot/SQLBridge"

[application]

config/name="AprenderAProgramar"
run/main_scene="res://Game/MainScreens/Splash.tscn"
config/icon="res://icon.png"

[autoload]

Main="*res://Game/Autoloads/Main.gd"
MusicManager="*res://Game/Autoloads/MusicManager.tscn"

[display]

window/size/width=1280
window/size/height=720
window/size/test_width=640
window/size/test_height=360
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "PersistenceNode", "RPGElements" )

[rendering]

environment/default_environment="res://default_env.tres"
